{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "items": {
    "type": "object",
    "properties": {
      "message": {
        "type": "string"
      },
      "keyword": {
        "type": "boolean",
        "default": true
      },
      "stickers": {
        "type": "boolean",
        "default": true
      },
      "reactions": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": ["message", "reactions"]
  }
}
